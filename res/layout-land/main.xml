<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent">

	<LinearLayout android:id="@+id/line_button"
		android:layout_width="fill_parent" android:layout_height="wrap_content">
		<CheckBox android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:id="@+id/CheckBox_full"
			android:text="全てチェック"></CheckBox>
		<Button android:text="カレンダーに登録" android:id="@+id/button_import"
			android:layout_width="wrap_content" android:layout_height="wrap_content"></Button>
	</LinearLayout>

	<ListView android:id="@+id/list" android:layout_width="wrap_content"
		android:layout_height="wrap_content"></ListView>

</LinearLayout>